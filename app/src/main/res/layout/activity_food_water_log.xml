<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".ui.lifestyle.FoodWaterLogActivity">

    <Button
        android:id="@+id/AddCalories"
        android:layout_width="147dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="10dp"
        android:background="#01579B"
        android:text="Track Food or Beverage"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/TrackCalories"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/TrackCalories"
        android:layout_width="131dp"
        android:layout_height="47dp"
        android:layout_marginStart="315dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="315dp"
        android:layout_marginBottom="370dp"
        android:background="#01579B"
        android:text="View Food Log"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AddCalories" />
</androidx.constraintlayout.widget.ConstraintLayout>